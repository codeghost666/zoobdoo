{% extends 'SonataAdminBundle:CRUD:edit.html.twig' %}

{% block side_bar_after_nav %}
{% endblock %}

{% set sonata_breadcrumb_last = object.property.name %}
{% block sonata_breadcrumb %}
    {% include 'ErpAdminBundle:blocks:sonata_breadcrumb.html.twig' %}
{% endblock %}

{% block notice %}
    {% include 'ErpAdminBundle:blocks:sonata_message.html.twig' %}
{% endblock notice %}

{% block navbar_title %}
    Edit {{ admin.label }}
{% endblock %}

{% block sonata_tab_content %}
    {{ parent() }}
    <div class="col-md-12">
        <div class="col-md-12">
            <div class="box">
                <div class="box-body">
                    <h4>Link</h4>
                    <a href="{{ path('erp_property_page', {'stateCode': object.property.city.stateCode, 'cityName': object.property.city.name, 'name':object.property.name}) }}" target="_blank">{{ object.property.name }}</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
